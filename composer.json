{
    "name": "adminintelligence/laravel-log-shipper",
    "description": "Ships your Laravel logs to a central server. Because local problems deserve global visibility.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AdminIntelligence\\LogShipper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "AdminIntelligence",
            "email": "dev@adminintelligence.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "monolog/monolog": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdminIntelligence\\LogShipper\\LogShipperServiceProvider"
            ]
        }
    }
}
